<script src="/Scripts/bootstrap-dialog.min.js"></script>
<script src="/Scripts/bootstrap-toggle.js"></script>
<script src="/Scripts/bootstrap-editable.js"></script>
<script src="/Scripts/bootstrap-table-editable.js"></script>
<script src="/vendor/plugins/daterange/daterangepicker.js"></script>
<script src="/vendor/plugins/tagmanager/tagmanager.js"></script>
<script src="/assets/js/utility/utility.js"></script>
<script src="/assets/js/main.js"></script>
<script src="/assets/js/demo.js"></script>
<script src="/vendor/plugins/ladda/ladda.min.js"></script>
<script src="/Scripts/bootstrap-datetimepicker.min/moment-with-locales.js"></script>
<script src="/Scripts/bootstrap-datetimepicker.min/moment-timezone-with-data-2010-2020.min.js"></script>
<script src="/Scripts/bootstrap-datetimepicker.min/bootstrap-datetimepicker.js"></script>
<script src="/Scripts/toastr.js"></script>
<script src="/Scripts/linq.min.js"></script>
<script src="/Scripts/jquery.mask.js"></script>
<script src="/Scripts/TrataDataJson.js"></script>
<script src="/Scripts/jquery.maskMoney.js"></script>
<script src="/Scripts/bootstrap-filestyle.js"></script>
<script src="/Scripts/waitMe.js"></script>
<script src="/Scripts/genericos/CarregandoLoadGif.js"></script>
<script src="/Scripts/genericos/BotaoDireitoMouse.js"></script>
<script src="/Scripts/genericos/NoBack.js"></script>
<script src="/Scripts/genericos/GerarRelatorio.js"></script>
<script src="/Scripts/jquery-migrate-1.0.0.js"></script>
<script src="/Scripts/accounting.js"></script>
<script src="/Scripts/numeral.js"></script>
<script src="/Scripts/numeral-pt-br.js"></script>
<script src="/Scripts/languages.js"></script>
<script src="/Scripts/genericos/TrataNumerico.js"></script>
<script src="/Scripts/timerjs.js"></script>
<script src="/Scripts/respond.js"></script>
<script src="/Scripts/genericos/ModalBootstrapDialog.js"></script>
<script src="/Scripts/genericos/GerandoTipoRelatorioBase.js"></script>
<script src="/Scripts/genericos/MetodosPadrao.js"></script>
<script src="/Scripts/Util/CarregarDataAtual.js"></script>
<script src="/Scripts/genericos/Ticket.js"></script>
<script src="/Scripts/genericos/Utils.js"></script>

<div class="content">
    <div class="row col-xs-12">
        <div class="col-md-12 filtro" style="margin-left:12px; margin-top:5px;">
            <div class="row">
                <div class="panel">
                    <div class="panel-heading">
                        <span class="panel-title"><i class="fa fa-search"></i>Filtrar resultado</span>
                        <span class="panel-title pull-right">
                            <a class="collapse-link">
                                <i class="fa fa-chevron-up"></i>
                            </a>
                        </span>
                    </div>
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-lg-2">
                                <div class="form-group">
                                    <label class="control-label" for="">Projeto</label>
                                    <select id="Projeto" name="Projeto" class="form-control input-sm"></select>
                                </div>
                            </div>
                            <div class="col-lg-4">
                                <div class="form-group">
                                    <label class="control-label" for="regime">Regime</label>
                                    <input type="text" class="form-control input-sm" id="regime" name="regime" />
                                </div>
                            </div>
                            <div class="col-lg-2">
                                <div class="form-group">
                                    <label class="control-label">Mês/Ano</label>
                                    <div class='input-group' id='mesAno'>
                                        <input type='text' class="form-control input-sm" />
                                        <span class="input-group-addon input-sm">
                                            <span class="fa fa-calendar"></span>
                                        </span>
                                    </div>
                                </div>
                                <script type="text/javascript">
                                    $("#mesAno").datetimepicker({
                                        locale: 'pt-br',
                                        format: 'MM/YYYY'
                                    });
                                </script>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-12">
                                <div class="form-group" style="float:right">
                                    
                                    <a href="javascript:void(0);" class="btn btn-primary btn-sm" id="btnPesquisar" name="btnPesquisar" title="Pesquisar" tabindex="1">
                                        <i class="fa fa-search"></i>&nbsp;Pesquisar
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="panel table-responsive">
                    <div class="panel-heading">
                        <span class="panel-title"><i class="fa fa-list"></i>&nbsp;Consulta</span>
                        <span class="panel-title pull-right">
                            <a class="collapse-link">
                                <i class="fa fa-chevron-up"></i>
                            </a>
                        </span>
                    </div>
                    <div class="panel-body">
                        <div class="form-group" style="float:right">
                            <a href="javascript:void(0);" class="btn btn-excel btn-sm" id="btnExcel" title="Exemplo de botão Excel" tabindex="6">
                                <i class="fa fa-file-excel-o"></i>&nbsp;Excel
                            </a>
                        </div>
                        
                        <table id="tblResultado"
                               data-toggle="table"
                               data-click-to-select="true"
                               data-pagination="true"
                               data-show-footer="false"
                               data-multi-select="true"
                               data-unique-id=""
                               data-page-list="[5, 10, 20, 50, 100, 200, 500, 1000, 10000]"
                               class="table table-hover">
                            <thead>
                                <tr>
                                    <th data-field="Nome" data-align="" data-sortable="true">Projeto</th>
                                    <th data-field="Regime" data-align="" data-sortable="true">Regime</th>
                                    <th data-field="HorasFormatado" data-align="" data-sortable="true">Horas lançadas</th>
                                    <th data-field="DiaUtil" data-align="" data-sortable="true">Dias Úteis</th>
                                    <th data-field="HoraUtil" data-align="" data-sortable="true">Horario Util</th>
                                    <th data-field="Mes" data-align="" data-sortable="true">Mês</th>
                                    <th data-field="Ano" data-align="" data-sortable="true">Ano</th>
                                    <th data-field="AssessoriaConsultaPJFormatado" data-align="" data-sortable="true">Ass. e Consul. - Pj</th>
                                    <th data-field="SalarioOrdenadoFormatado" data-align="" data-sortable="true">Salarios e Ordenados</th>
                                    <th data-field="HoraExtraFormatado" data-align="" data-sortable="true">Horas Extras Normais</th>
                                    <th data-field="FeriasFormatado" data-align="" data-sortable="true">Ferias</th>
                                    <th data-field="DecimoTerceiroProvisaoFormatado" data-align="" data-sortable="true">Prov. 13 Salário</th>
                                    <th data-field="INSSFormatado" data-align="" data-sortable="true">INSS</th>
                                    <th data-field="INSSProvisaoFeriasFormatado" data-align="" data-sortable="true">INSS Prov. Ferias</th>
                                    <th data-field="INSSProvisaoDecimoTerceiroFormatado" data-align="" data-sortable="true">INSS Prov. 13</th>
                                    <th data-field="FGTSFormatado" data-align="" data-sortable="true">FGTS</th>
                                    <th data-field="FGTSProvisaoFeriasFormatado" data-align="" data-sortable="true">FGTS Prov. Ferias</th>
                                    <th data-field="FGTSProvisaoDecimoTerceiroFormatado" data-align="" data-sortable="true">FGTS Prov. 13</th>
                                    <th data-field="MedicaFormatado" data-align="" data-sortable="true">Assist. Med.</th>
                                    <th data-field="OdontoFormatado" data-align="" data-sortable="true">Assist. Odonto.</th>
                                    <th data-field="PensaoFormatado" data-align="" data-sortable="true">Fundo De Pensão</th>
                                    <th data-field="TaxaAdmPensaoFormatado" data-align="" data-sortable="true">Taxa Adm. Fundo De Pensão</th>
                                    <th data-field="SeguroFormatado" data-align="" data-sortable="true">Seguro De Vida Em Grupo</th>
                                    <th data-field="VtFormatado" data-align="" data-sortable="true">Vale Transporte</th>
                                    <th data-field="VrFormatado" data-align="" data-sortable="true">Vale Refeição</th>
                                    <th data-field="CestaFormatado" data-align="" data-sortable="true">Cesta Básica</th>
                                    <th data-field="CrecheFormatado" data-align="" data-sortable="true">Auxilio Creche</th>
                                    <th data-field="VaFormatado" data-align="" data-sortable="true">Vale Alimentação</th>
                                    <th data-field="BabaFormatado" data-align="" data-sortable="true">Auxilio Baba</th>

                                </tr>
                            </thead>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


@section scripts
{

    <script src="~/Scripts/ScriptsViews/Controle/Index.js?v= @(Guid.NewGuid())"></script>
}